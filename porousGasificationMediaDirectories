### porousGasificationFoam install and running directory
### as a default is is set to ~/OpenFoam/<user>-8/applications/porousGasificationMedia

DIR=$WM_PROJECT_USER_DIR/applications/porousGasificationMedia

### end of user editable part ###

export FOAM_HGS=$DIR
export LIB_HGS_DIR=$FOAM_HGS/lib
export LD_LIBRARY_PATH=$LIB_HGS_DIR:$LD_LIBRARY_PATH
export POROUS_DOC_SRC=$(pwd)

### end of porousGasificationFoam install and running directory
