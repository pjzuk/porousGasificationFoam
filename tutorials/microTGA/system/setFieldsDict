FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
);

regions
(
    boxToCell
    {
        box (-0.002 -0.002 -0.002) (0.002 0.002 0.002);
        fieldValues
        (
            volScalarFieldValue porosityF 0.55 
        );
    }
);


